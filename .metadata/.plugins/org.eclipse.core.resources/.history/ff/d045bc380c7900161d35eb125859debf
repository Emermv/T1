#include "Persona.h"

Persona::Persona(string codigo,string nombre,string apellidos,string genero,int edad){
	this->codigo=codigo;
	this->nombre=nombre;
	this->apellidos=apellidos;
	this->genero=genero;
	this->edad=edad;
}
string Persona::getCodigo(){
	return this->codigo;
}
